{
	"name": "typescript-react",
	"description": "Parcel TypeScript simple example",
	"version": "1.0.0",
	"main": "src/index.html",
	"license": "MIT",
	"scripts": {
		"start": "parcel src/index.html --open --no-hmr --port 1234",
		"devMobile": "parcel src/index.html --no-hmr --port 1234 --no-source-maps",
		"type-check": "tsc --noEmit",
		"type-check:watch": "tsc --noEmit --watch",
		"save-json": "node node/node.js",
		"build": "rm -rf dist/*.*  && parcel build src/index.html --no-source-maps --public-url ./ --out-dir dist",
		"copy": "cp ../python/data/write/*.json ./dist/data"
	},
	"dependencies": {
		"@types/axios": "^0.14.0",
		"@types/dat.gui": "^0.7.5",
		"axios": "^0.19.2",
		"dat.gui": "^0.7.6",
		"firebase": "^7.5.0",
		"global": "^4.4.0",
		"gsap": "^3.0.1",
		"http-server": "^0.12.3",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-redux": "^7.1.3",
		"redux": "^4.0.4",
		"redux-devtools-extension": "^2.13.8",
		"redux-logger": "^3.0.6",
		"three": "^0.110.0",
		"three-orbitcontrols-ts": "^0.1.2",
		"tslint-config-prettier": "^1.18.0"
	},
	"devDependencies": {
		"@types/react": "^16.9.13",
		"@types/react-dom": "^16.9.4",
		"@types/react-redux": "^7.1.5",
		"@types/redux-logger": "^3.0.7",
		"glslify-bundle": "^5.1.1",
		"glslify-deps": "^1.3.1",
		"jsonfile": "^6.0.1",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.23.7",
		"tslint": "^5.20.1",
		"tslint-microsoft-contrib": "^6.2.0",
		"tslint-react": "^4.1.0",
		"tsutils": "^3.17.1",
		"typescript": "^3.7.2"
	}
}
